    "#Checker la taille pour et l'atmosphere max\n",
    "def atm(Size):\n",
    "    atmo = random.randint(0, 12) -7 + Size\n",
    "    if atmo <0:\n",
    "        atmo =0\n",
    "    return atmo\n",
    "\n",
    "def Size():\n",
    "    R=random.randint(0,10)\n",
    "    return R\n",
    "\n",
    "def density(diam_digit):\n",
    "    diametre=[800,1600,3200,4800,6400,8000,\n",
    "              9600,11200,12800,14400,16000]#en Km\n",
    "    diam = diametre[diam_digit]\n",
    "    dens = random.randint(0,12)\n",
    "    if diam >= 38226:\n",
    "        dens -= 1\n",
    "    elif diam >= 63710:\n",
    "        dens -= 2\n",
    "    elif diam >= 127420:\n",
    "        dens -= 2\n",
    "    return dens\n",
    "\n",
    "def Gravity(diam_digit,densindex):\n",
    "    diametre=[800,1600,3200,4800,6400,8000,\n",
    "              9600,11200,12800,14400,16000]#en Km\n",
    "    dens_list = [0.05, 0.25, 0.5, 0.6, 0.80, 0.90, 0.95, 1, 1, 1.05, 1.05, 1.2, 1.4, 1.5]\n",
    "\n",
    "    grav = dens_list[densindex]*diam_digit\n",
    "    return grav\n",
    "\n",
    "#nouvelle fonction\n",
    "def pre_Hydrography(atmosphere):\n",
    "    hydrographie = random.randint(0,10)\n",
    "    if (atmosphere == 0 or atmosphere==1 or atmosphere==10 or atmosphere==11 or atmosphere==12):\n",
    "        hydrographie-=4\n",
    "    return hydrographie\n",
    "\n",
    "#Inutilisé\n",
    "def Hydrography_final(hydro, temp):\n",
    "    hydrographie = hydro\n",
    "    #temp = temp - 270\n",
    "    #print(\"Temperature = \"+str(temp))\n",
    "    if temp.item() >= 100:\n",
    "        hydrographie-=6\n",
    "    if temp.item() > 20 and temp.item() <100:\n",
    "        hydrographie -= 2\n",
    "    if(hydrographie<=0):\n",
    "        hydrographie=0\n",
    "    return hydrographie\n",
    "\n",
    "def Biomass(atmo,val_o,statut_orbit,climat):\n",
    "    #val_o = hydrographie\n",
    "    #statut orbit = lettre 'H','I','O'\n",
    "    biom = random.randint(0,12)\n",
    "    #print(\"climat = \"+ str(climat))\n",
    "    if statut_orbit =='O':\n",
    "        biom -= 2\n",
    "    elif statut_orbit == 'I':\n",
    "        biom += 2\n",
    "    if atmo == 1 or atmo == 11 or atmo == 14:\n",
    "        biom -= 4\n",
    "    elif atmo == 2 or atmo == 3:\n",
    "        biom-=3\n",
    "    elif atmo==4 or atmo ==5 or atmo == 10  or atmo ==13:\n",
    "        biom -=2\n",
    "    elif atmo == 8 or atmo ==9:\n",
    "        biom+=2\n",
    "    elif atmo ==12:\n",
    "        biom-=6\n",
    "    if val_o ==0:\n",
    "        biom -= 4\n",
    "    elif val_o in range(6,9):\n",
    "        biom+=1\n",
    "    elif val_o == 9 or val_o ==10:\n",
    "        biom+=2\n",
    "    if climat.item() >= 100 or climat.item() <= -100:#modifier ça\n",
    "        biom-=4\n",
    "    return biom\n",
    "\n",
    "def Biodiversity(Biomasse):\n",
    "    return random.randint(0, 12)-7 + Biomasse\n",
    "    #voir la liste de 2300ad pour les habitabillity modifiers\n",
    "\n",
    "def Biocomplexity(Biodiversite):\n",
    "    return random.randint(0, 12)-7 + Biodiversite\n",
    "\n",
    "def Compatibility(Biomasse, Biodiversite, Biocomplexite, atmosphere):\n",
    "    Biosphere = (Biomasse + Biodiversite + Biocomplexite)/3\n",
    "    compatibilite = random.randint(0,12) + 7 - Biosphere\n",
    "    if atmosphere == 10:\n",
    "        compatibilite -= 6\n",
    "    if atmosphere == 11:\n",
    "        compatibilite -= 8\n",
    "    if atmosphere == 12:\n",
    "        compatibilite -= 10\n",
    "    return compatibilite\n",
    "\n",
    "def natural_ressources(diam_digit, atmosphere, hydrographie, biodiversite):\n",
    "    ressources = random.randint(0,12) - 7 + diam_digit\n",
    "    if atmosphere in range(0,1):\n",
    "        ressources -= 2\n",
    "    if atmosphere in range(4,9):\n",
    "        ressources += 1\n",
    "    if hydrographie in range(4,7):\n",
    "        ressources += 1\n",
    "    if hydrographie in range(9,10):\n",
    "        ressources -= 2\n",
    "    if biodiversite >= 7 :\n",
    "        ressources += 1\n",
    "    return ressources\n",
    "\n",
    "def habitability(hydrographie,temperature, gravite, compatibilite, biodiversite):#nécessite température\n",
    "    #temperature -= 270\n",
    "    habitabilite = 10\n",
    "\n",
    "    if(compatibilite >= 10):\n",
    "        habitabilite += 2\n",
    "    \n",
    "    #modif biodiversite\n",
    "    if(biodiversite <= 0):\n",
    "        habitabilite -= 4\n",
    "    if(biodiversite in [1,2]):\n",
    "        habitabilite -= 2\n",
    "    if(biodiversite in [3,4,5,6]):\n",
    "        habitabilite += 1\n",
    "    if(biodiversite in [7,8]):\n",
    "        habitabilite += 2\n",
    "    if(biodiversite >= 10):\n",
    "        habitabilite -= 4\n",
    "\n",
    "    #modif hydrographie\n",
    "    if(hydrographie<=0):\n",
    "        habitabilite-=4\n",
    "    if(hydrographie in [1,2,3]):\n",
    "        habitabilite-= 2\n",
    "    if(hydrographie== 10):\n",
    "        habitabilite-= 2 \n",
    "    \n",
    "    #modif temperature\n",
    "    if(temperature.item() >= 10 or temperature.item() <= 25):\n",
    "        habitabilite+=2\n",
    "    if(temperature.item() >= 100 or temperature.item() <= -100):\n",
    "        habitabilite-=4\n",
    "    elif(temperature.item() >= 60 or temperature.item() <= -60):\n",
    "        habitabilite-=2\n",
    "    \n",
    "    \n",
    "    #modif gravite\n",
    "    #print(\"Gravity falls : \"+str(gravite))\n",
    "    if(gravite <= 0.3): #micro gravite\n",
    "        habitabilite -= 4\n",
    "    if(gravite <= 0.7 and gravite > 0.3): #faible gravite\n",
    "        habitabilite += 2\n",
    "    if(gravite <= 2 and gravite > 1.5):\n",
    "        habitabilite -= 2\n",
    "    if(gravite <= 5 and gravite > 2):\n",
    "        habitabilite -= 4\n",
    "    if(gravite > 5):\n",
    "        habitabilite -= 6\n",
    "    return habitabilite"
